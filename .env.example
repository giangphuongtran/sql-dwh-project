# 📦 PostgreSQL Configuration
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=your_database_name

# 🌐 pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=your_admin_email
PGADMIN_DEFAULT_PASSWORD=your_admin_password

# 🔗 ETL + Connection Settings
PG_USER=your_db_user
PG_PASSWORD=your_db_password
PG_HOST=your_db_host
PG_PORT=your_db_port # Default is 5432 for PostgreSQL on Docker
PG_DB=your_database_name

# ⚙️ Optional Settings
RUN_INGEST=false # Set to true to run ingestion tasks

# Note: Adjust RUN_INGEST based on your ETL workflow needs